<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#fff">

    <ImageButton
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:id="@+id/imageButtonEnergy"
        android:scaleType="fitXY"
        android:background="#ffffff"
        android:maxHeight="95dp"
        android:maxWidth="95dp"
        android:minHeight="95dp"
        android:minWidth="95dp"

        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="23dp"
        android:layout_marginStart="23dp"
        android:layout_marginTop="35dp"
        android:src="@drawable/energy" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButtonArea"
        android:scaleType="fitXY"
        android:background="#ffffff"
        android:maxHeight="95dp"
        android:maxWidth="95dp"
        android:minHeight="95dp"
        android:minWidth="95dp"
        android:layout_alignBottom="@+id/imageButtonEnergy"
        android:src="@drawable/area"
        android:layout_alignRight="@+id/imageButtonLength"
        android:layout_alignEnd="@+id/imageButtonLength"
        android:layout_alignLeft="@+id/imageButtonLength"
        android:layout_alignStart="@+id/imageButtonLength"
        android:layout_alignTop="@+id/imageButtonEnergy" />

    <ImageButton
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:id="@+id/imageButtonData"
        android:scaleType="fitXY"
        android:background="#ffffff"
        android:maxHeight="95dp"
        android:maxWidth="95dp"
        android:minHeight="95dp"
        android:minWidth="95dp"
        android:src="@drawable/data"
        android:layout_alignTop="@+id/imageButtonArea"
        android:layout_toRightOf="@+id/imageButtonArea"
        android:layout_toEndOf="@+id/imageButtonArea"
        android:layout_marginLeft="25dp"
        android:layout_marginStart="21dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Energy"
        android:textSize="16dp"
        android:id="@+id/textView2"
        android:layout_below="@+id/imageButtonEnergy"
        android:layout_alignRight="@+id/imageButtonEnergy"
        android:layout_alignEnd="@+id/imageButtonEnergy"
        android:layout_alignLeft="@+id/imageButtonEnergy"
        android:layout_alignStart="@+id/imageButtonEnergy"
        android:textAlignment="center" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Area"
        android:textSize="16dp"
        android:id="@+id/textView3"
        android:layout_below="@+id/imageButtonArea"
        android:layout_alignLeft="@+id/imageButtonArea"
        android:layout_alignStart="@+id/imageButtonArea"
        android:layout_toStartOf="@+id/imageButtonData"
        android:textAlignment="center"
        android:layout_toLeftOf="@+id/imageButtonData" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Data"
        android:textSize="16dp"
        android:id="@+id/textView4"
        android:layout_below="@+id/imageButtonData"
        android:layout_alignRight="@+id/imageButtonData"
        android:layout_alignEnd="@+id/imageButtonData"
        android:layout_alignLeft="@+id/imageButtonData"
        android:layout_alignStart="@+id/imageButtonData"
        android:textAlignment="center" />

    <ImageButton
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:id="@+id/imageButtonFrequency"
        android:scaleType="fitXY"
        android:background="#ffffff"
        android:maxHeight="95dp"
        android:maxWidth="95dp"
        android:minHeight="95dp"
        android:minWidth="95dp"
        android:src="@drawable/fequency"
        android:layout_below="@+id/textView2"
        android:layout_alignLeft="@+id/imageButtonEnergy"
        android:layout_alignStart="@+id/imageButtonEnergy"
        android:layout_marginTop="22dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Frequency"
        android:textSize="16dp"
        android:id="@+id/textView5"
        android:layout_below="@+id/imageButtonFrequency"
        android:layout_alignLeft="@+id/imageButtonFrequency"
        android:layout_alignStart="@+id/imageButtonFrequency"
        android:layout_alignRight="@+id/imageButtonFrequency"
        android:layout_alignEnd="@+id/imageButtonFrequency"
        android:textAlignment="center" />

    <ImageButton
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:id="@+id/imageButtonLength"
        android:scaleType="fitXY"
        android:background="#ffffff"
        android:src="@drawable/length"
        android:maxHeight="95dp"
        android:maxWidth="95dp"
        android:minHeight="95dp"
        android:minWidth="95dp"
        android:layout_above="@+id/textView7"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Length"
        android:textSize="16dp"
        android:id="@+id/textView6"
        android:layout_below="@+id/imageButtonLength"
        android:layout_alignRight="@+id/imageButtonLength"
        android:layout_alignEnd="@+id/imageButtonLength"
        android:layout_alignLeft="@+id/imageButtonLength"
        android:layout_alignStart="@+id/imageButtonLength"
        android:textAlignment="center" />

    <ImageButton
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:id="@+id/imageButtonPressure"
        android:scaleType="fitXY"
        android:background="#ffffff"
        android:src="@drawable/pressure"
        android:maxHeight="95dp"
        android:maxWidth="95dp"
        android:minHeight="95dp"
        android:minWidth="95dp"
        android:layout_alignTop="@+id/imageButtonLength"
        android:layout_alignLeft="@+id/imageButtonData"
        android:layout_alignStart="@+id/imageButtonData" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pressure"
        android:textAlignment="center"
        android:textSize="16dp"
        android:id="@+id/textView7"
        android:layout_above="@+id/imageButtonWeight"
        android:layout_alignRight="@+id/imageButtonPressure"
        android:layout_alignEnd="@+id/imageButtonPressure"
        android:layout_alignLeft="@+id/imageButtonPressure"
        android:layout_alignStart="@+id/imageButtonPressure" />

    <ImageButton
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:id="@+id/imageButtonWeight"
        android:scaleType="fitXY"
        android:background="#fff"
        android:maxHeight="95dp"
        android:maxWidth="95dp"
        android:minHeight="95dp"
        android:minWidth="95dp"
        android:src="@drawable/weight"
        android:layout_marginTop="26dp"
        android:layout_below="@+id/textView5"
        android:layout_alignLeft="@+id/imageButtonFrequency"
        android:layout_alignStart="@+id/imageButtonFrequency" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Weight"
        android:textSize="16dp"
        android:id="@+id/textView8"
        android:layout_below="@+id/imageButtonWeight"
        android:layout_alignRight="@+id/textView2"
        android:layout_alignEnd="@+id/textView2"
        android:layout_alignLeft="@+id/imageButtonWeight"
        android:layout_alignStart="@+id/imageButtonWeight"
        android:textAlignment="center" />

    <ImageButton
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:id="@+id/imageButtonVolume"
        android:scaleType="fitXY"
        android:background="#fff"
        android:maxHeight="95dp"
        android:maxWidth="95dp"
        android:minHeight="95dp"
        android:minWidth="95dp"
        android:src="@drawable/volume"
        android:layout_alignBottom="@+id/imageButtonWeight"
        android:layout_alignLeft="@+id/imageButtonLength"
        android:layout_alignStart="@+id/imageButtonLength" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Volume"
        android:textSize="16dp"
        android:id="@+id/textView9"
        android:layout_below="@+id/imageButtonVolume"
        android:layout_alignLeft="@+id/textView6"
        android:layout_alignStart="@+id/textView6"
        android:layout_alignRight="@+id/imageButtonVolume"
        android:layout_alignEnd="@+id/imageButtonVolume"
        android:textAlignment="center" />

    <ImageButton
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:id="@+id/imageButtonTemp"
        android:scaleType="fitXY"
        android:background="#fff"
        android:maxHeight="95dp"
        android:maxWidth="95dp"
        android:minHeight="95dp"
        android:minWidth="95dp"
        android:src="@drawable/temperature"
        android:layout_alignBottom="@+id/imageButtonVolume"
        android:layout_alignLeft="@+id/imageButtonPressure"
        android:layout_alignStart="@+id/imageButtonPressure" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Temperature"
        android:textSize="16dp"
        android:id="@+id/textView10"
        android:layout_below="@+id/imageButtonTemp"
        android:layout_alignLeft="@+id/imageButtonTemp"
        android:layout_alignStart="@+id/imageButtonTemp"
        android:layout_alignRight="@+id/imageButtonTemp"
        android:layout_alignEnd="@+id/imageButtonTemp"
        android:textAlignment="center" />

    <com.google.android.gms.ads.AdView
        android:id="@+id/adView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        ads:adSize="SMART_BANNER"
        ads:adUnitId="@string/banner_ad_unit_id">
    </com.google.android.gms.ads.AdView>




</RelativeLayout>